{
  "adminPassword": "admin",
  "cacheDefaultObject": "default",
  "caches": {
    "default": {
      "class": "lucee.runtime.cache.ram.RamCache",
      "custom": {
        "timeToIdleSeconds": "0",
        "timeToLiveSeconds": "0"
      },
      "readOnly": "false",
      "storage": "false",
      "type": "RAM"
    }
  },
  "datasources": {
    "basecfc": {
      "class": "org.postgresql.Driver",
      "custom": "stringtype=unspecified",
      "database": "basecfc",
      "dbdriver": "PostgreSql",
      "dsn": "jdbc:postgresql://{host}:{port}/{database}/?{custom}",
      "host": "localhost",
      "password": "test",
      "port": "5432",
      "username": "test"
    }
  },
  "inspectTemplate": "always",
  "localScopeMode": "modern",
  "robustExceptionEnabled": true,
  "scopeCascading": "strict"
}